{
  "name": "benchmark",
  "description": "No description",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "/tmp/checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
  {
    "name": "in-kafka",
    "type": "Kafka",
    "configuration": {
      "topics": [{"topic": "benchmark", "partition" : "1"}],
      "zookeeper.connect": [{"host": "localhost", "port": "2181"}],
      "kafkaParams.group.id": "KafkaCluster"
    }
  }
,
  "transformations": [
    {
      "name": "morphline-parser",
      "inputField": "attachment_body",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        {"name":"orderId"},
        {"name":"timestamp"},
        {"name":"clientId"},
        {"name":"latitude"},
        {"name":"longitude"},
        {"name":"paymentMethod"},
        {"name":"creditCard"},
        {"name":"shoppingCenter"},
        {"name":"employee"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "orderId": "/order_id",
                  "timestamp": "/timestamp",
                  "clientId": "/client_id",
                  "latitude": "/latitude",
                  "longitude": "/longitude",
                  "paymentMethod": "/payment_method",
                  "creditCard": "/credit_card",
                  "shoppingCenter": "/shopping_center",
                  "employee": "/employee"
                }
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "outputFields": [
        {"name":"timestamp"}
      ],
      "configuration": {
        "inputFormat": "autoGenerated"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "writer": {
        "outputs" : ["out-print"]
      },
      "dimensions": [
        {
          "name": "payment_method",
          "field": "paymentMethod"
        },
        {
          "name": "shopping_center",
          "field": "shoppingCenter"
        },
        {
          "name": "minute",
          "field": "timestamp",
          "type": "DateTime",
          "precision": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {

      }
    }
  ]
}
