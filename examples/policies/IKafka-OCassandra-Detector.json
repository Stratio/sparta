{
  "name": "KafkaToCassandra",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
  {
    "name": "in-kafka",
    "type": "Kafka",
    "configuration": {
      "topics": "det",
      "kafkaParams.zookeeper.connect": "localhost:2181",
      "kafkaParams.group.id": "testgroup",
      "storageLevel": "MEMORY_AND_DISK_SER_2"
    }
  }
,
  "transformations": [
    {
      "name": "detector-parser",
      "order": 1,
      "outputFields": [
        "recorded_at_ms",
        "company_root",
        "ou_vehicle",
        "geo",
        "alarm_code",
        "asset",
        "path_id",
        "rpm_event_avg",
        "odometer"
      ],
      "type": "Detector",
      "configuration": {}
    },
    {
      "name": "recorded_at_ms-parser",
      "inputField": "recorded_at_ms",
      "outputFields": [
        "recorded_at",
        "recorded_at2"
      ],
      "order": 2,
      "type": "DateTime",
      "configuration": {
        "inputFormat": "unixMillis"
      }
    }
  ],
  "cubes": [
    {
      "name": "rpmAvg",
      "checkpointConfig": {
        "timeDimension": "reco",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000000000000
      },
      "dimensions": [
        {
          "name": "companyRoot",
          "field": "company_root"
        },
        {
          "name": "ouVehicle",
          "field": "ou_vehicle"
        },
        {
          "name": "reco",
          "field": "recorded_at",
          "type": "DateTime",
          "precision": "minute"
        },
        {
          "name": "reco2",
          "field": "recorded_at2",
          "type": "DateTime",
          "precision": "minute",
          "configuration":{
            "typeOp":"datetime"
          }
        }
      ],
      "operators": [
        {
          "name": "avg_rpm",
          "type": "Avg",
          "configuration": {
            "inputField": "rpm_event_avg"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-cassandra",
      "type": "Cassandra",
      "configuration": {
        "connectionHost": "127.0.0.1",
        "connectionPort": "9042",
        "cluster": "Test Cluster",
        "keyspace": "sparkta"
      }
    }
  ]
}