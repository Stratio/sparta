{
  "name": "policy-rabbit",
  "description": "Input rabbit, output print",
  "sparkStreamingWindow": 2000,
  "rawData": {
    "enabled": "false",
    "path": "myTestParquetPath"
  },
  "checkpointPath": "checkpoint",
  "input":
  {
    "name": "in",
    "type": "RabbitMQ",
    "configuration": {
      "queue": "test",
      "host": "localhost",
      "port": "5672",
      "exchangeName": "twitterExchange",
      "routingKeys": [{"routingKey": "routingKey1"}]
    }
  }
,
  "transformations": [
    {
      "name": "morphline",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        "appName",
        "timestamp",
        "geo",
        "tag",
        "twitterStatus"
      ],
      "configuration": {
        "morphline": {
          "id": "test1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "appName": "/appName",
                  "timestamp": "/timestamp",
                  "geo": "/geo",
                  "tag": "/tag",
                  "twitterStatus": "/twitterStatus"
                }
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "inputField": "timestamp",
      "outputFields": [
        "timestamp"
      ],
      "configuration": {
        "inputFormat": "unixMillis"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "checkpointConfig": {
        "timeDimension": "minute",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000
      },
      "dimensions": [
        {
          "field": "appName",
          "name": "appName"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {}
    }
  ]
}