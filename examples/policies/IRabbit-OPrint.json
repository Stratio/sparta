{
  "name": "policy-rabbit",
  "description": "Input rabbit, output print",
  "sparkStreamingWindow": 2000,
  "rawData": {
    "enabled": "false",
    "path": "myTestParquetPath"
  },
  "input":
  {
    "name": "in",
    "type": "RabbitMQ",
    "configuration": {
      "queue": "test",
      "host": "localhost",
      "port": "5672",
      "exchangeName": "twitterExchange",
      "routingKeys": [{"routingKey": "routingKey1"}]
    }
  }
,
  "transformations": [
    {
      "name": "morphline",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        {"name":"appName"},
        {"name":"timestamp"},
        {"name":"geo"},
        {"name":"tag"},
        {"name":"twitterStatus"}
      ],
      "configuration": {
        "morphline": {
          "id": "test1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "appName": "/appName",
                  "timestamp": "/timestamp",
                  "geo": "/geo",
                  "tag": "/tag",
                  "twitterStatus": "/twitterStatus"
                }
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "inputField": "timestamp",
      "outputFields": [
        {"name":"timestamp"}
      ],
      "configuration": {
        "inputFormat": "unixMillis"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "writer": {
        "outputs" : ["out-print"]
      },
      "dimensions": [
        {
          "field": "appName",
          "name": "appName"
        },
        {
          "name": "minute",
          "field": "timestamp",
          "type": "DateTime",
          "precision": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {}
    }
  ]
}