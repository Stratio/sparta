{
  "name": "policy-Twitter-ElasticSearch",
  "sparkStreamingWindow": 6000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-twitter",
      "type": "Twitter",
      "configuration": {
        "consumerKey": "*****",
        "consumerSecret": "*****",
        "accessToken": "*****",
        "accessTokenSecret": "*****"
      }
    }
  ,
  "cubes": [
    {
      "name": "testCube",
      "checkpointConfig": {
        "timeDimension": "minute",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000
      },
      "dimensions": [
        {
          "name": "hashtags",
          "field": "status",
          "type": "TwitterStatus",
          "precision": "hashtags"
        },
        {
          "name": "firsthashtag",
          "field": "status",
          "type": "TwitterStatus",
          "precision": "firsthashtag"
        },
        {
          "name": "retweets",
          "field": "status",
          "type": "TwitterStatus",
          "precision": "retweets"
        },
        {
          "name": "userLocation",
          "field": "userLocation"
        },
        {
          "name": "precision3",
          "field": "geolocation",
          "type": "GeoHash",
          "precision": "precision3"
        },
        {
          "name": "minute",
          "field": "timestamp",
          "type": "DateTime",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        },
        {
          "name": "distinctoperator",
          "type": "Count",
          "configuration": {
            "distinctFields": "wordsN"
          }
        },
        {
          "name": "sumoperator",
          "type": "Sum",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "maxoperator",
          "type": "Max",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "minoperator",
          "type": "Min",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "avgoperator",
          "type": "Avg",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "medianoperator",
          "type": "Median",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "varianceoperator",
          "type": "Variance",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "stddevoperator",
          "type": "Stddev",
          "configuration": {
            "inputField": "wordsN"
          }
        },
        {
          "name": "fullTextoperator",
          "type": "FullText",
          "configuration": {
            "inputField": "userLocation"
          }
        },
        {
          "name": "lastValueoperator",
          "type": "LastValue",
          "configuration": {
            "inputField": "retweets"
          }
        },
        {
          "name": "accumulatoroperator",
          "type": "Accumulator",
          "configuration": {
            "inputField": "wordsN"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-elasticsearch",
      "type": "ElasticSearch",
      "configuration": {
        "nodes": "localhost",
        "defaultPort": "9200",

        "isAutoCalculateId": "false",
        "dateType": "timestamp",
        "indexMapping": "day"
      }
    }
  ]
}