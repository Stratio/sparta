{
  "name": "policy-websocket",
  "description": "Input kafka, output print",
  "sparkStreamingWindow": 2000,
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
  {
    "name": "in-websocket",
    "type": "WebSocket",
    "configuration": {
      "url": "ws://stream.meetup.com/2/rsvps"
    }
  },
  "transformations": [
    {
      "name": "morphline",
      "type": "Morphlines",
      "order": 0,
      "outputFields": [
        {"name":"response"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readJson": {}
            },
            {
              "extractJsonPaths": {
                "paths": {
                  "response": "/response"
                }
              }
            }
          ]
        }
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "writer": {
        "outputs" : ["out-solr"]
      },
      "dimensions": [
        {
          "field": "response",
          "name": "response"
        }
      ],
      "operators": [
        {
          "name": "countoperator",
          "type": "Count",
          "configuration": {}
        }
      ]
    }
    ],

  "outputs": [
    {
      "name": "out-solr",
      "type": "Solr",
      "configuration": {
        "connection": "localhost:2181",
        "createSchema": "true",
        "isCloud": "true",
        "cloudDataDir": "/tmp/solr-data"
        //"fixedMeasure": "_indexed_at_tdt:0"
      }
    }]
}
