{
  "name": "ATSocketCassandra",
  "description": "No description",
  "sparkStreamingWindow": 5000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "path": ""
  },
  "input":
  {
    "name": "in-socket",
    "type": "Socket",
    "configuration": {
      "hostname": "localhost",
      "port": "10666"
    }
  }
,
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        {"name":"product"},
        {"name":"price"},
        {"name":"text"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readCSV": {
                "separator": ",",
                "columns": [
                  "product",
                  "price",
                  "text"
                ],
                "trim": "true",
                "charset": "UTF-8",
                "quoteChar": "\"",
                "commentPrefix": "#"
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "outputFields": [
        {"name":"timestamp", "type": "long"}
      ],
      "configuration": {
        "inputFormat": "autoGenerated"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCubeWithTime",
      "writer": {
        "outputs" : ["out-cassandra"]
      },
      "dimensions": [
        {
          "name": "product",
          "field": "product"
        },
        {
          "name": "minute",
          "field": "timestamp",
          "type": "DateTime",
          "precision": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "avg_price",
          "type": "Mean",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "count_price",
          "type": "Count",
          "configuration": {}
        },
        {
          "name": "first_price",
          "type": "FirstValue",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "fulltext_price",
          "type": "FullText",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "last_price",
          "type": "LastValue",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "max_price",
          "type": "Max",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "median_price",
          "type": "Median",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "min_price",
          "type": "Min",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "range_price",
          "type": "Range",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "stddev_price",
          "type": "Stddev",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "sum_price",
          "type": "Sum",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "variance_price",
          "type": "Variance",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "entitycount_text",
          "type": "EntityCount",
          "configuration": {
            "inputField": "text",
            "split": " "
          }
        },
        {
          "name": "totalentity_text",
          "type": "TotalEntityCount",
          "configuration": {
            "inputField": "text",
            "split": " "
          }
        }
      ]
    },
    {
      "name": "testCubeWithoutTime",
      "writer": {
        "outputs" : ["out-cassandra"]
      },
      "dimensions": [
        {
          "name": "product",
          "field": "product"
        }
      ],
      "operators": [
        {
          "name": "avg_price",
          "type": "Mean",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "count_price",
          "type": "Count",
          "configuration": {}
        },
        {
          "name": "first_price",
          "type": "FirstValue",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "fulltext_price",
          "type": "FullText",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "last_price",
          "type": "LastValue",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "max_price",
          "type": "Max",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "median_price",
          "type": "Median",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "min_price",
          "type": "Min",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "range_price",
          "type": "Range",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "stddev_price",
          "type": "Stddev",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "sum_price",
          "type": "Sum",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "variance_price",
          "type": "Variance",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "entitycount_text",
          "type": "EntityCount",
          "configuration": {
            "inputField": "text",
            "split": " "
          }
        },
        {
          "name": "totalentity_text",
          "type": "TotalEntityCount",
          "configuration": {
            "inputField": "text",
            "split": " "
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-cassandra",
      "type": "Cassandra",
      "configuration": {
        "connectionHost": "127.0.0.1",
        "connectionPort": "9142",
        "cluster": "Test Cluster",
        "keyspace": "sparta",
        "sparkProperties": [
          {
            "sparkPropertyKey": "spark.cassandra.connection.keep_alive_ms",
            "sparkPropertyValue": "180000"
          }
        ]
      }
    }
  ]
}
