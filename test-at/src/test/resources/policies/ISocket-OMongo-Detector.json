{
  "name": "ATSocketMongoDetector",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-socket",
      "type": "Socket",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ,
  "transformations": [
    {
      "name": "detector-parser",
      "order": 1,
      "outputFields": [
        "recorded_at_ms",
        "company_root",
        "ou_vehicle",
        "geo",
        "alarm_code",
        "asset",
        "path_id",
        "rpm_event_avg",
        "odometer"
      ],
      "type": "Detector",
      "configuration": {}
    },
    {
      "name": "recorded_at_ms-parser",
      "inputField": "recorded_at_ms",
      "outputFields": [
        "recorded_at_ms"
      ],
      "order": 2,
      "type": "DateTime",
      "configuration": {
        "inputFormat": "unixMillis"
      }
    }
  ],
  "cubes": [
    {
      "name": "rpmAvg",
      "checkpointConfig": {
        "timeDimension": "recordedAtMs",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000000000000
      },
      "dimensions": [
        {
          "name": "companyRoot",
          "field": "company_root"
        },
        {
          "name": "ouVehicle",
          "field": "ou_vehicle"
        },
        {
          "name": "geo",
          "field": "geo",
          "type": "GeoHash",
          "precision": "precision3"
        },
        {
          "name": "recordedAtMs",
          "field": "recorded_at_ms",
          "type": "DateTime",
          "precision": "minute"
        }
      ],
      "operators": [
        {
          "name": "avg_rpm",
          "type": "Avg",
          "configuration": {
            "inputField": "rpm_event_avg"
          }
        }
      ]
    },
    {
      "name": "maxMinOdometer",
      "checkpointConfig": {
        "timeDimension": "recordedAtMs",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000000000000
      },
      "dimensions": [
        {
          "name": "companyRoot",
          "field": "company_root"
        },
        {
          "name": "ouVehicle",
          "field": "ou_vehicle"
        },
        {
          "name": "asset",
          "field": "asset"
        },
        {
          "name": "recordedAtMs",
          "field": "recorded_at_ms",
          "type": "DateTime",
          "precision": "minute"
        },
        {
          "name": "pathId",
          "field": "path_id"
        }
      ],
      "operators": [
        {
          "name": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "odometer"
          }
        },
        {
          "name": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "odometer"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-mongo",
      "type": "MongoDb",
      "configuration": {
        "hosts": "localhost:60000",
        "dbName": "csvtest",
        "multiplexer": "false"
      }
    }
  ]
}