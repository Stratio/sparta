{
  "id": "id",
  "name": "ATSocketParquet",
  "description": "No description",
  "sparkStreamingWindow": 2000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "input":
    {
      "name": "in-socket",
      "type": "Socket",
      "jarFile": "input-socket-plugin.jar",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ,
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "type": "Morphlines",
      "jarFile" : "parser-morphlines-plugin.jar",
      "outputFields": [
        "product",
        "price",
        "text"
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readCSV": {
                "separator": ",",
                "columns": [
                  "product",
                  "price",
                  "text"
                ],
                "trim": "true",
                "charset": "UTF-8",
                "quoteChar": "\"",
                "commentPrefix": "#"
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "jarFile" : "parser-datetime-plugin.jar",
      "outputFields": [
        "timestamp"
      ],
      "configuration": {
        "inputFormat": "autoGenerated"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "checkpointConfig": {
        "timeDimension": "minute",
        "granularity": "minute",
        "interval": 30000,
        "timeAvailability": 60000,
        "jarFile": "field-dateTime-plugin.jar"
      },
      "dimensions": [
        {
          "name": "product",
          "field": "product",
          "jarFile": "field-default-plugin.jar"
        }
      ],
      "operators": [
        {
          "name": "acc_price",
          "type": "Accumulator",
          "jarFile" : "operator-accumulator-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "avg_price",
          "type": "Avg",
          "jarFile" : "operator-avg-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "count_price",
          "type": "Count",
          "jarFile" : "operator-count-plugin.jar",
          "configuration": {}
        },
        {
          "name": "first_price",
          "type": "FirstValue",
          "jarFile" : "operator-firstValue-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "fulltext_price",
          "type": "FullText",
          "jarFile" : "operator-fullText-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "last_price",
          "type": "LastValue",
          "jarFile" : "operator-lastValue-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "max_price",
          "type": "Max",
          "jarFile" : "operator-max-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "median_price",
          "type": "Median",
          "jarFile" : "operator-median-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "min_price",
          "type": "Min",
          "jarFile" : "operator-min-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "range_price",
          "type": "Range",
          "jarFile" : "operator-range-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "stddev_price",
          "type": "Stddev",
          "jarFile" : "operator-stddev-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "sum_price",
          "type": "Sum",
          "jarFile" : "operator-sum-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "variance_price",
          "type": "Variance",
          "jarFile" : "operator-variance-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "mode_price",
          "type": "Mode",
          "jarFile" : "operator-mode-plugin.jar",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "entityCount_text",
          "type": "EntityCount",
          "jarFile" : "operator-entityCount-plugin.jar",
          "configuration": {
            "inputField": "text",
            "split": " "
          }
        },
        {
          "name": "totalEntity_text",
          "type": "TotalEntityCount",
          "jarFile" : "operator-totalEntityCount-plugin.jar",
          "configuration": {
            "inputField": "text",
            "split": " "
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-parquet",
      "type": "Parquet",
      "jarFile" : "output-parquet-plugin.jar",
      "configuration": {
        "path": "/tmp/sparkta/operators/parquet",
        "datePattern": "yyyy/MM/dd",
        "multiplexer": "false"
      }
    }
  ]
}