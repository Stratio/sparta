{
  "name": "ATSocketParquet",
  "sparkStreamingWindow": 2000,
  "rawData": {
    "enabled": "false",
    "partitionFormat": "day",
    "path": "myTestParquetPath"
  },
  "checkpointing": {
    "path": "checkpoint",
    "timeDimension": "minute",
    "granularity": "minute",
    "interval": 30000,
    "timeAvailability": 60000
  },
  "inputs": [
    {
      "name": "in-socket",
      "type": "Socket",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ],
  "fields": [
    {
      "name": "product"
    },
    {
      "name": "price"
    },{
      "name": "timestamp",
      "type": "DateTime",
      "configuration": {
        "typeOp": "String"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "dimensions": [
        {
          "dimension": "product"
        }
      ],
      "operators": [
        {
          "measureName": "accum-operator",
          "type": "Accumulator",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "avg-operator",
          "type": "Avg",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "count-operator",
          "type": "Count",
          "configuration": {
          }
        },
        {
          "measureName": "first-value-operator",
          "type": "FirstValue",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "full-text-operator",
          "type": "FullText",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "last-value-operator",
          "type": "LastValue",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "max-operator",
          "type": "Max",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "median-operator",
          "type": "Median",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "min-operator",
          "type": "Min",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "range-operator",
          "type": "Range",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "stddev-operator",
          "type": "Stddev",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "sum-operator",
          "type": "Sum",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "measureName": "variance-operator",
          "type": "Variance",
          "configuration": {
            "inputField": "price"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-parquet",
      "type": "Parquet",
      "configuration": {
        "path": "/tmp/sparkta/operators/parquet",
        "datePattern": "yyyy/MM/dd",
        "multiplexer": "false"
      }
    }
  ],
  "parsers": [
    {
      "name": "morphline-parser",
      "type": "Morphlines",
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readCSV": {
                "separator": ",",
                "columns": [
                  "product",
                  "price"
                ],
                "trim": "true",
                "charset": "UTF-8",
                "quoteChar": "\"",
                "commentPrefix": "#"
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "type": "DateTime",
      "configuration": {
        "timestamp": "autoGenerated"
      }
    }
  ]
}