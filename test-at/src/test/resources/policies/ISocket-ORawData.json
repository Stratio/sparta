{

  "name": "ATRawData",
  "description": "No description",
  "sparkStreamingWindow": 5000,
  "checkpointPath": "checkpoint",
  "rawData": {
    "enabled": "true",
    "path": "checkpoint/sparta/raw"
  },
  "input":
    {
      "name": "in-socket",
      "type": "Socket",
      "configuration": {
        "hostname": "localhost",
        "port": "10666"
      }
    }
  ,
  "transformations": [
    {
      "name": "morphline-parser",
      "order": 0,
      "type": "Morphlines",
      "outputFields": [
        {"name":"product"},
        {"name":"price"}
      ],
      "configuration": {
        "morphline": {
          "id": "morphline1",
          "importCommands": [
            "org.kitesdk.**"
          ],
          "commands": [
            {
              "readCSV": {
                "separator": ",",
                "columns": [
                  "product",
                  "price"
                ],
                "trim": "true",
                "charset": "UTF-8",
                "quoteChar": "\"",
                "commentPrefix": "#"
              }
            }
          ]
        }
      }
    },
    {
      "name": "timestamp",
      "order": 1,
      "type": "DateTime",
      "outputFields": [
        {"name":"timestamp", "type": "long"}
      ],
      "configuration": {
        "inputFormat": "autoGenerated"
      }
    }
  ],
  "cubes": [
    {
      "name": "testCube",
      "writer": {
        "outputs" : ["out-print"]
      },
      "dimensions": [
        {
          "name": "product",
          "field": "product"
        },
        {
          "name": "minute",
          "field": "timestamp",
          "type": "DateTime",
          "precision": "minute",
          "computeLast": "10m"
        }
      ],
      "operators": [
        {
          "name": "sum-operator",
          "type": "Sum",
          "configuration": {
            "inputField": "price"
          }
        },
        {
          "name": "avg-operator",
          "type": "Mean",
          "configuration": {
            "inputField": "price"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "out-print",
      "type": "Print",
      "configuration": {}
    }
  ]
}